/* component/selectUpload.wxss */
@import "../../../app.wxss";
.mask-warp{
  width: 100%;
  height: 440rpx; 
  background: #fff;
  position: absolute;
  bottom: 0;
  text-align: center;
}
.mask-warp1{
  height: 504rpx;
}
.creat-type{
  text-align: center;
  margin-top: 48rpx;
  font-family: PingFang-SC-Medium;
  color: #888888;
}
.chioce-type{
  margin-top: 32rpx;
  padding: 32rpx 40rpx 0;
}
.chioce-warp{
   text-align: center;
}
.chioce1{
  margin-left: 40rpx;
}
.chioce3{
  margin-right: 40rpx;
}
.chioce-image{
  display: block;
  border-radius: 16rpx;
  height: 80rpx;
  width: 80rpx;
  margin: 0 auto;
}
.chioce-text{
  display: block;
  font-family: PingFang-SC-Medium;
  color: #888888;
  letter-spacing: 0.24rpx;
  margin-top: 10rpx;
}
.cancel{
  position: absolute;
  bottom: 0;
  height: 100rpx;
  width: 100%;
  line-height: 100rpx;
  border-top: 2rpx solid #EEE;
  text-align: center;
  font-family: PingFang-SC-Medium;
  color: #353535;
}


