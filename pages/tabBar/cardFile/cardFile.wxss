@import "../../../app.wxss";
/* 头部 */
.head{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  padding:  0 48rpx;
  font-size: 32rpx;
  box-sizing: border-box;
}
.back-map{
  height: 50rpx;
  line-height: 50rpx;
  border: 1px solid #e2e2e2;
  padding: 8rpx;
  font-size: 24rpx;
  color: #e2e2e2;

}

/* 列表名 */
.tab{
  width: 100%;
  height: 80rpx;
  margin-top: 10rpx;
  background: #fff;
  font-size: 28rpx;
  color: #e2e2e2;
}
.class::after {
  margin-right: 10rpx;
  content: '\e680';
  font-family: 'iconfont';
  font-size: 24rpx;
}