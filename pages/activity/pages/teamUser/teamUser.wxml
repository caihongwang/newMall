<view style='width:100%; height:100%;background:#fff;'>
    <view style='overflow: hidden;'>
        <!-- <scroll-view class='warp' scroll-y="true" bindscrolltolower="bindMore" > -->
        <view class="team-name-view">- {{teamName}} -</view>
        <view class="p-body">
            <block wx:for='{{teamUserList}}' wx:key='uid'>
                <view class="item">
                   <view class="image-view-warp">
                     <image class="image-view" src='{{item.avatarUrl}}'>
                    </image>
                     <view class="leader-view" wx:if='{{index==0}}'>团长</view>
                   </view>
                    <view class="nickname">{{item.nickname}}</view>
                </view>
            </block>
        </view>
        <!-- <</scroll-view> -->
    </view>
    <view class='foot'>
        <view class='add-more fz-20' wx:if="{{isShowMore}}">上拉加载更多</view>
        <view class='add-more fz-20' wx:if="{{loading}}">加载中，请稍后~</view>

        <view class='add-more fz-20' wx:if="{{isNoShowMore}}">没有更多</view>
    </view>
    <view class="{{isIpx ? 'iphonex':''}}"></view>


</view>