/* pages/activity/pages/teamIntegralDetail/teamIntegralDetail.wxss */
@import "../../../../app.wxss";
/* 团队积分明细-本团 */
.my-team{
  width: 100%;
  /* height: 1100rpx; */
  height: 100%;
  background: #fff;
}
.team-total{
font-family: PingFangSC-Semibold;
font-size: 40rpx;
color: #7965F5;
letter-spacing: 0;
text-align: center;
padding: 42rpx 0 8rpx 0;
}
.team-head{
  font-family: PingFangSC-Regular;
color: #7964F5;
letter-spacing: 0;
text-align: center;
line-height: 24rpx;
}
.team-integral{
  width: 228rpx;
  height: 228rpx;
  line-height: 228rpx;
  text-align: center;
  margin: 32rpx auto 56rpx;
  background-image: linear-gradient(-139deg, #9DC7FF 2%, #9B8DFF 100%);
  box-shadow: 0 0 4rpx 4rpx rgba(255,255,255,0.20), 0 0 4px 6px rgba(218,226,255,0.50);
  font-family: PingFangSC-Semibold;
  border-radius: 114rpx;
  font-size: 60rpx;
  color: #FFFFFF;
  letter-spacing: 0;
}
.line{
  border-bottom: 2rpx dashed  rgba(121,100,244,0.40);
}
.line-express{
  font-family: PingFangSC-Light;
  color: #C9C1FB;
  letter-spacing: 0;
  line-height: 24rpx;
}
.not-prize-warp{
  padding: 36rpx 64rpx 0;
  background: #fff;
}
.not-prize{
  height: 96rpx;
  border-bottom: 2rpx solid rgba(121,100,244,0.10);
}
.ranking-not{
  color: #4F4F4F;
  width: 80rpx;
  text-align: center;
}
.not-image-warp{
  width: 68rpx;
  height: 68rpx;
  margin-right: 24rpx;
  position: relative;
}
.ranking-not-image1{
  width: 68rpx;
  height: 68rpx;
  border-radius: 34rpx;
}
.wx-name-not{
  width: 280rpx;
  font-family: PingFang-SC-Medium;
color: #4F4F4F;
letter-spacing: 0;
line-height: 28rpx;
text-overflow:ellipsis;
white-space:nowrap;
overflow:hidden;
}
.wx-name-not-number{
  width: 180rpx;
  font-family: PingFang-SC-Medium;
color: #4F4F4F;
letter-spacing: 0;
text-align: right;
line-height: 40px;
}
.wx-name-not-text{
  font-family: PingFangSC-Light;
color: #4F4F4F;
letter-spacing: 0;
text-align: right;
line-height: 24rpx;
}
.not-prize-warp .not-prize:last-child {
  border: 0
}
.my-integral{
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #7965F5;
  letter-spacing: 0;
  line-height: 32rpx;
  text-align: center;
  margin-bottom: 70rpx;
}
.my-integral-text{
  margin: 0 28rpx 0 12rpx;
  font-family: PingFangSC-Semibold;
font-size: 52rpx;
color: #7965F5;
letter-spacing: 0;
line-height: 40rpx;
}
.add-more{
  opacity: 0.4;
  font-family: PingFangSC-Regular;
  color: #9D9D9D;
  letter-spacing: 0;
  text-align: center;
  /* margin-top: 30rpx; */
}
.foot{
  height: 106rpx;
  line-height: 106rpx;
  width: 100%;
  background: #fff;
}
.iphonex{
  width: 100%;
  height: 68rpx;
  background: #fff;
}

.leader-view {
  background: #FFCA6A;
  border: 2rpx solid #FFFFFF;
  border-radius: 200rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #CB8A15;
  letter-spacing: 0;
  width: 56rpx;
  position: absolute;
  right: -6rpx;
  top: 0;
}