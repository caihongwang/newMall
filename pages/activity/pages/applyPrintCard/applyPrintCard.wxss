.image-load{
    width: 100%;
    height: 100%;
  }
  .card-image-loading{
    display: block;
    width: 96rpx;
    height: 96rpx;
    padding-top: 100rpx;
    margin: 0 auto;
  }
  .loading-text{
    width: 100%;
    text-align: center;
    margin-top: 20rpx;
    color: #D8D8D8;
    font-size: 24rpx;
  }
  .common-btnstyle {
    color: #fff;
    font-size: 36rpx;
    height: 96rpx;
    line-height: 96rpx;
    margin: 60rpx 60rpx;
    border-radius: 200rpx;
    font-family: PingFangSC-Semibold;

}
.submit-button {
    background-image: linear-gradient(90deg,#FF7060  0%,#FFD36B  100%);
