}

.color-white {
  color: white;
}

.activity-color-bg {
  background: red;
}

.label-cart-bar {
  border: 0rpx red solid;
  color: #878787;
  font-size: 12px;
  float: left;
  border-left: 4px solid #4296f7;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: center; /** flex 属性， 水平居中**/
}

.label-cart {
  padding: 6px 5px 6px 5px;
}

.icon-clear {
  border: 0rpx red solid;
  color: #878787;
  float: right;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: center; /** flex 属性， 水平居中**/
  padding-right: 25rpx;
}

.label-clear {
  color: #878787;
  font-size: 12px;
}

.cart-list-box {
  background: #fff;
