.page {
  overflow-y: hidden;
  width: 100%;
  height: 100%;
}

.list-left-menu {
  width: 160rpx;
  position: absolute;
  left: 0px;
  z-index: 10;
}

.list-left-menu-box-unselect {
  padding: 10rpx;
  border-left: 3px solid #f9f9f9;
  border-bottom: 1px solid #b2b2b2;
  height: 72rpx;
  color: black;
  font-weight: bold;
  background: #f9f9f9;
}

.list-left-menu-box-selected {
  padding: 10rpx;
  height: 72rpx;
  color: #4296f7;
  font-weight: bold;
  border-left: 3px solid #4296f7;
  background: white;
}

.list-tab {
  border: 0rpx red solid;
  width: 100%;
  height: 40rpx;
  background: rgb(249, 249, 250);
  color: #b2b2b2;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: left; /** flex 属性， 水平居中**/
  margin-left: 160rpx;
  padding-left: 10rpx;
}

.list-menu-name {
  text-align: center;
  margin-top: 14rpx;
}

.content-list {
  border: 0rpx red solid;
  width: 100%;
  height: 180rpx;
  margin-left: 160rpx;
  display: flex;
  border-bottom: 1px solid #e3e3e3;
}

.list-image-box {
  border: 0rpx green solid;
  width: 180rpx;
  height: 180rpx;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: center; /** flex 属性， 水平居中**/
}

.list-image {
  border: 0rpx red solid;
  width: 160rpx;
  height: 160rpx;
}

.issue-name {
  border: 0rpx red solid;
  width: 54%;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: left; /** flex 属性， 水平居中**/
}

.itemNameAndPriceView {
  border: 0rpx green solid;
  width: 100%;
  height: 180rpx;
}

.itemNameView {
  border: 0rpx green solid;
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-weight: bold;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: left; /** flex 属性， 水平居中**/
}

.itemDegistiView {
  border: 0rpx green solid;
  width: 100%;
  height: 55rpx;
  line-height: 27rpx;
  color: #b2b2b2;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: left; /** flex 属性， 水平居中**/
}

.itemNumView {
  border: 0rpx green solid;
  width: 100%;
  height: 30rpx;
  line-height: 25rpx;
  color: #b2b2b2;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: left; /** flex 属性， 水平居中**/
}

.itemPriceAndPlusView {
  border: 0rpx green solid;
  width: 100%;
  height: 50rpx;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: left; /** flex 属性， 水平居中**/
}

.itemPriceText {
  border: 0rpx green solid;
  width: 50%;
  text-align: left;
  color: #ff4c4b;
  font-weight: bold;
}

.plusView {
  border: 0rpx green solid;
  width: 50%;
  height: 100%;
  float: right;
  padding-right: 15rpx;
}

.plus-icon {
  float: right;
  color: #4296f7;
  font-size: 46rpx;
}

.operate-bar {
  z-index: 1001;
  position: absolute;
  bottom: 0px;
  height: 55px;
  width: 100%;
  display: flex;
}

.gouwuche {
  border: 0rpx red solid;
  width: 75%;
  height: 100%;
  background: #353535;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: left; /** flex 属性， 水平居中**/
  padding-left: 20rpx;
}

.gouwuche-iconfont {
  font-family: "iconfont" !important;
  font-size: 40px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.gouwuche-icon {
  border: 0rpx red solid;
  color: #87888e;
  width: 80rpx;
  height: 80rpx;
}

.number-msg {
  border: 0rpx red solid;
  padding: 1px 7px;
  border-radius: 50%;
  background: #ff9c35;
  color: white;
  font-size: 14px;
  position: absolute;
  text-align: center;
  top: 5px;
  left: 35px;
}

.gouwuche-price {
  border: 0rpx red solid;
  color: #a9a9a9;
  display: flex;
  align-items: center;
  margin-left: 15px;
}

.submit-btn {
  height: 100%;
  background: #4296f7;
  width: 25%;
  display: flex;
}

.submit-btn-label {
  color: white;
  font-weight: bold;
  margin: auto;
}

.drawer_screen {
  border: 0rpx red solid;
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}

.drawer_box_ad {
  margin-left: 7%;
  width: 86%;
  overflow: hidden;
  position: fixed;
  top: 15%;
  z-index: 1001;
  background: #fff;
  border-radius: 4px;
}

.cartlist-float {
  width: 100%;
  overflow: hidden;
  position: fixed;
  z-index: 1001;
  bottom: 55px;
  max-height: 250px;
}

.drawer_content_ad {
  /* height: 289px; */
  width: 100%;
  padding: 10px;
}

.optionsView{
  border: 0rpx green solid;
  margin-top: 5rpx;
}

.itemNameView {
  border: 0rpx green solid;
  width: 100%;
  height: 40rpx;
