.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.go-center {
  display: flex;
  justify-content: center;
}

.down-center {
  display: flex;
  align-items: center;
}

.flex-display {
