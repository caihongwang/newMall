}

.font20 {
  font-size: 20px;
}

.top-bar {
  height: 40px;
  font-size: 14px;
  background: white;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: center; /** flex 属性， 水平居中**/
  padding: 0rpx 20rpx;
}

.top-left-label {
  border: 0rpx red solid;
  width: 45%;
  float: left;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: left; /** flex 属性， 水平居中**/
}

.dataAndTimeView {
  border: 0rpx red solid;
  width: 55%;
  float: right;
  color: red;
  font-weight: bold;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: center; /** flex 属性， 水平居中**/
}

.dateView{
  border: 0rpx red solid;
}

.dateShowView{
  border: 0rpx red solid;
  width: 250rpx;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: center; /** flex 属性， 水平居中**/
}
.dateLogoImg{
  width: 30rpx;
  height: 30rpx;
}

.dateText{
  padding-left: 10rpx;
}


.timeView{
  border: 0rpx red solid;
}

.timeShowView{
  border: 0rpx red solid;
  align-items: center; /** 垂直居中*/
  display: flex;
  justify-content: center; /** flex 属性， 水平居中**/
}
.timeLogoImg{
  width: 40rpx;
  height: 40rpx;
}

.timeText{
  padding-left: 5rpx;
}


.order-info {
  background: white;
  margin-top: 10px;
}

.order-info-title {
  font-size: 12px;
  color: #d1d1d1;
  padding: 12px;
  border-bottom: 1px #e3e3e3 solid;
}

.order-cut {
  height: 30px;
  padding: 5px 15px;
  border-bottom: 1px #e3e3e3 solid;
}

.order-cut-dec {
  background: #f07474;
  font-size: 13px;
  color: white;
  padding: 2px;
}

.order-cut-note {
  margin-left: 3px;
  font-size: 14px;
}

.order-cut-number {
  font-size: 14px;
  float: right;
}

.order-sum {
  height: 30px;
  padding: 5px 15px;
  font-size: 14px;
}

.order-sum-number {
  font-size: 14px;
  float: right;
}

.note {
  padding: 5px 15px;
  background: white;
  margin-top: 10px;
}

.note-text {
  width: 95%;
  font-size: 12px;
  background: #f2f2f2;
  padding: 10px;
  height: 80px;
}

